{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "FileOne": {
      "FilePath": "logs/app.log",
      "FileWriteOption": 5,
      "MinLevel": "Information",
      "FileLimitBytes": 0,
      "MaxRollingFiles": 0
    },
    "FileTwo": {
      "FilePath": "logs/app_error.log",
      "FileWriteOption": 5,
      "MinLevel": "Error",
      "FileLimitBytes": 0,
      "MaxRollingFiles": 0
    }
  },
  "AllowedHosts": "*"
}
